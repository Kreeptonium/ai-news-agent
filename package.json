{
  "name": "ai-news-agent",
  "version": "0.0.1",
  "description": "An autonomous agent for scraping and sharing AI/tech news",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config=jest.integration.config.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "update-progress": "ts-node scripts/update-progress.ts"
  },
  "private": true,
  "dependencies": {
    "@elizaos/core": "latest",
    "@elizaos/agent": "latest",
    "@elizaos/agent-twitter-client": "latest",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}